---
import Title from "@components/Title.astro";
import IconoCheck from "@iconos/IconoCheck.astro";
import IconoSquare from "@iconos/IconoSquare.astro";
import IconoHouse from "@iconos/IconoHouse.astro";

const titleSectionProps = {
    titulo: "Consulta de Fichas de inmuebles religiosos",
    descripcion:
        "A continuación, se muestra la lista de los inmuebles religiosos catalogados del Centro Histórico de Cusco, en los cuales podremos encontrar su información arquitectonica y arqueológica, dividas en fichas de catalogación.",
